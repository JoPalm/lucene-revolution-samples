            <stringProp name="filename"></stringProp>
            <stringProp name="script">String hasChanged = props.get(&quot;hasChanged&quot;);

//log.info(&quot;Has Changed (indexing group): &quot; + hasChanged);
if (&quot;yes&quot;.equals(hasChanged)) {
	int targetLatency = 2000;

	int currentLatency = Integer.parseInt(props.get(&quot;latency&quot;));
	log.info(&quot;Current latency(indexing group): &quot; + String.valueOf(currentLatency));
	
	int currentThroughput = Integer.parseInt(vars.get(&quot;targetThroughput&quot;));
	log.info(&quot;Current throughput: &quot; + String.valueOf(currentThroughput));
	
	// logic of setting throughput
	int desiredThroughput = currentThroughput * targetLatency / currentLatency;
	
	log.info(&quot;Theoretical desired throughput: &quot; + String.valueOf(desiredThroughput));
	
	// upper and lower bounds
	if (desiredThroughput &gt; 6000) {
		desiredThroughput = 6000;
	}
	
	if (desiredThroughput &lt; 1) {
		desiredThroughput = 1;
	}
	
	log.info(&quot;Actual desired throughput: &quot; + String.valueOf(desiredThroughput));
	
	vars.put(&quot;targetThroughput&quot;, String.valueOf(desiredThroughput));
	props.put(&quot;hasChanged&quot;, &quot;no&quot;);
}</stringProp>
